{
  "name": "@getbigger-io/prisma-fixtures-cli",
  "version": "1.0.4",
  "description": "Prisma2 Fixtures Loader (adapted from typeorm-fixtures-cli)",
  "main": "dist/index.js",
  "bin": {
    "fixtures": "dist/cli.js"
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "trash dist",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "lint:fix": "prettier src/**/*.ts --write && tslint --fix --project .",
    "lint": "tslint --project . && prettier src/**/*.ts --list-different",
    "test": "nyc mocha",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "commit": "npx git-cz"
  },
  "keywords": [
    "prisma",
    "fixtures",
    "orm",
    "cli",
    "typescript",
    "faker",
    "fixture-loader",
    "prisma-fixtures"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getbigger-io/prisma-fixtures.git"
  },
  "author": "Nicolas MACHEREY <nicolas.macherey@gmail.com>",
  "bugs": {
    "url": "https://github.com/getbigger-io/prisma-fixtures/issues"
  },
  "homepage": "https://github.com/getbigger-io/prisma-fixtures#readme",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@prisma/cli": "^2.13.1",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chalk": "^2.2.0",
    "@types/cli-progress": "^3.7.0",
    "@types/commander": "^2.12.2",
    "@types/ejs": "^3.0.4",
    "@types/faker": "^4.1.11",
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.12.3",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/resolve-from": "^5.0.0",
    "@types/yargs-parser": "^15.0.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.21",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.2.6",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "trash-cli": "^3.0.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.5.3",
    "yaml-lint": "^1.2.4"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@prisma/client": "^2.13.1",
    "@types/hapi__joi": "^17.1.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "child_process": "^1.0.2",
    "class-transformer": "^0.3.1",
    "cli-progress": "^3.0.0",
    "commander": "^5.1.0",
    "ejs": "^3.1.3",
    "faker": "^4.1.0",
    "glob": "^7.1.6",
    "js-yaml": "3.14.0",
    "lodash": "^4.17.14",
    "reflect-metadata": "^0.1.13",
    "resolve-from": "^5.0.0",
    "yargs-parser": "^18.1.3"
  },
  "peerDependencies": {
    "@prisma/client": "^2.13.1"
  }
}
